module fizzbuzz;

extern fn printf(var string text var i32 num) void;

fn main() void {
  var i32 num = 5;

  if (num % 3) == 0 {
    printf("Fizz" 0);
  }

  if (num % 5) == 0 {
    printf("Buzz" 0);
  } 

  if ((num % 3) != 0) and ((num % 5) != 0) {
    printf("%d" num);
  }

  printf("\n" 0);
  return;
}
